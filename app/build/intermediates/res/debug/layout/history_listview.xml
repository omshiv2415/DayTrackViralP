<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="5dp"
    android:layout_marginRight="5dp"
    android:background="@android:drawable/dialog_holo_light_frame">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Start Time                 :"
        android:id="@+id/textView"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="29dp"
        android:layout_marginTop="10dp"
        android:textColor="@android:color/background_dark" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="11:00 AM"
        android:id="@+id/stattime"
        android:layout_alignTop="@+id/textView"
        android:layout_toEndOf="@+id/textView"
        android:layout_marginLeft="34dp"
        android:textColor="@android:color/background_dark" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Finish Time               :"
        android:id="@+id/textView7"
        android:layout_below="@+id/textView"
        android:layout_alignStart="@+id/textView"
        android:textColor="@android:color/background_dark" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="12:00 PM"
        android:id="@+id/finsihtime"
        android:layout_alignTop="@+id/textView7"
        android:layout_alignStart="@+id/stattime"
        android:textColor="@android:color/background_dark" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Calories Burn            :"
        android:id="@+id/textView8"
        android:layout_below="@+id/textView7"
        android:layout_alignStart="@+id/textView7"
        android:textColor="@android:color/background_dark" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="21"
        android:id="@+id/caloriesburn"
        android:layout_below="@+id/finsihtime"
        android:layout_alignStart="@+id/finsihtime"
        android:textColor="@android:color/background_dark" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Elasped Time            :"
        android:id="@+id/textView9"
        android:layout_below="@+id/textView8"
        android:layout_alignStart="@+id/textView8"
        android:textColor="@android:color/background_dark" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1:00 Hour"
        android:id="@+id/elapsedTime"
        android:layout_below="@+id/caloriesburn"
        android:layout_alignStart="@+id/caloriesburn"
        android:textColor="@android:color/background_dark" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Steps Taken              :"
        android:id="@+id/textView10"
        android:layout_below="@+id/textView9"
        android:layout_alignStart="@+id/textView9"
        android:textColor="@android:color/background_dark" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="21"
        android:id="@+id/stepstaken"
        android:layout_below="@+id/elapsedTime"
        android:layout_alignStart="@+id/elapsedTime"
        android:textColor="@android:color/background_dark" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Distance Covered     :"
        android:id="@+id/textView11"
        android:layout_marginBottom="10dp"
        android:layout_below="@+id/textView10"
        android:layout_alignStart="@+id/textView10"
        android:textColor="@android:color/background_dark" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0.21 Mile"
        android:id="@+id/distancecovered"
        android:layout_alignTop="@+id/textView11"
        android:layout_alignStart="@+id/stepstaken"
        android:textColor="@android:color/background_dark" />
</RelativeLayout><!-- From: file:/C:/BSC%20COMPUTING%20SYSTEMS/Final%20Submission%2060%20Marks/dayTrackViralP2/app/src/main/res/layout/history_listview.xml -->